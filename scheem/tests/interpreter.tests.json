[
	{
		"suite" : "quote",
		"tests" : [
			{
				"description" 	: "a number",
				"test" 			: ["quote", 3],
				"env"  			: {},
				"expected" 		: 3
			},
			{
				"description"	: "an atom",
				"test"			: ["quote", "dog"],
				"env"				: {},
				"expected"		: "dog"
			},
			{
				"description"	: "a list",
				"test"			: ["quote", [1, 2, 3]],
				"env"				: {},
				"expected"		: [1, 2, 3]
			}
		]
	},
	{
		"suite" : "add",
		"tests" : [
			{
				"description" 	: "without args",
				"test" 			: ["+"],
				"env"  			: {}
			},
			{
				"description" 	: "5 numbers",
				"test" 			: ["+", 1, 2, 3, 4, 5],
				"env"  			: {},
				"expected"		: 15
			},
			{
				"description" 	: "two numbers",
				"test" 			: ["+", 3, 5],
				"env"  			: {},
				"expected" 		: 8
			},
			{
				"description"	: "a number and an expression",
				"test"			: ["+", 3, ["+", 2, 2]],
				"env"				: {},
				"expected"		: 7
			},
			{
				"description"	: "a dog and a cat",
				"test"			: ["+", "dog", "cat"],
				"env"				: {}
			},
			{
				"description"	: "two plus two",
				"test"			: ["+", 2, 2],
				"env"				: {},
				"expected"		: 4
			}
		]
	},
	{
		"suite" : "div",
		"tests" : [
			{
				"description" : " 1 / 0 ",
				"test" : ["/", 1, 0],
				"env" : {}
			}
		]
	},
	{
		"suite" : "=",
		"tests" : [
			{
				"description" : "one equals one",
				"test" : ["=", 1, 1],
				"env" : {},
				"expected" : "#t"
			},
			{
				"description" : "one equals two",
				"test" : ["=", 1, 2],
				"env" : {},
				"expected" : "#f"
			}
		]
	},
	{
		"suite" : "car",
		"tests" : [
			{
				"description" : "Normal list",
				"test" : ["car", ["quote", ["a", "b", "c"]]],
				"env" : {},
				"expected" : "a"
			},
			{
				"description" : "Empty list",
				"test" : ["car", ["quote", []]],
				"env" : {}
			},
			{
				"description" : "More than enough!",
				"test" : ["car", ["quote", ["b"]], ["quote", ["a"]]],
				"env" : {}
			},
			{
				"description" : "Not a list!",
				"test" : ["car", ["+", 2, 2]],
				"env" : {}
			},
			{
				"description" : "No args!",
				"test" : ["car"],
				"env" : {}
			}
		]
	},
	{
		"suite" : "cdr",
		"tests" : [
			{
				"description" : "Normal list",
				"test" : ["cdr", ["quote", ["a", "b", "c"]]],
				"env" : {},
				"expected" : ["b", "c"]
			},
			{
				"description" : "Empty list",
				"test" : ["cdr", ["quote", []]],
				"env" : {}
			},
			{
				"description" : "More than enough!",
				"test" : ["cdr", ["quote", ["b"]], ["quote", ["a"]]],
				"env" : {}
			},
			{
				"description" : "Not a list!",
				"test" : ["cdr", ["+", 2, 2]],
				"env" : {}
			},
			{
				"description" : "No args!",
				"test" : ["cdr"],
				"env" : {}
			},
			{
				"description" : "List with no tail!",
				"test" : ["cdr", ["quote", ["a"]]],
				"env" : {},
				"expected" : []
			}
		]
	},
	{
		"suite" : "if",
		"tests" : [
			{
				"description" : "add in true branch",
				"test" : ["if", ["=", 1, 1], ["+", 2, 2], ["+", 5, 5]],
				"env"  : {},
				"expected" : 4
			},
			{
				"description" : "add in false branch",
				"test" : ["if", ["=", 1, 2], ["+", 2, 2], ["+", 5, 5]],
				"env"  : {},
				"expected" : 10
			},
			{
				"description" : "atom in true branch",
				"test" : ["if", ["=", 1, 1], ["quote", "duck"], ["quote", "pig"]],
				"env"  : {},
				"expected" : "duck"
			},
			{
				"description" : "atom in false branch",
				"test" : ["if", ["=", 1, 2], ["quote", "duck"], ["quote", "pig"]],
				"env"  : {},
				"expected" : "pig"
			},
			{
				"description" : "Too much args!",
				"test" : ["if", ["=", 1, 2], ["quote", "duck"], ["quote", "pig"], ["quote", "duck"]],
				"env"  : {}
			},
			{
				"description" : "Not enough args!",
				"test" : ["if", ["=", 1, 2], ["quote", "duck"]],
				"env"  : {}
			},
			{
				"description" : "No args!",
				"test" : ["if"],
				"env"  : {}
			}
		]
	}
]
